	<parameter name="dataInicial" class="java.lang.String"/>
	<parameter name="dataFinal" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT dt_atendimento, paciente.nome as paciente, estado_paciente, medico.nome as medico, TIME_FORMAT(CONCAT('',TIMEDIFF( dt_atendimento,dt_agendamento )),'%H:%i') AS espera
FROM atendimento
INNER JOIN paciente ON paciente_id = paciente.id
INNER JOIN medico ON medico_id = medico.id
INNER JOIN plano_saude ON plano_saude_id = plano_saude.id
WHERE dt_atendimento BETWEEN $P{dataInicial} AND $P{dataFinal}
ORDER BY dt_atendimento DESC]]>
	</queryString>
